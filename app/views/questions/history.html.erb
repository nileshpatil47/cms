<%= app_title("Question History")%>

<!--start: Wrapper-->
<div id="wrapper">
  <!--start: Container -->
  <div class="container">
    <!--start: Row -->
    <div class="row">
      <div class="span8">
      	<% @question.versions.each_with_index do |version, index| %>
          <% question = version.reify %>
          <div id="about">
            <p>
              <%= question.title if question %>
            </p>
          </div>
        <% end %>
      </div>
      <div class="span4">
        <!-- start: Sidebar -->
        <%= render "klass/sidebar" %>
        <!-- end: Sidebar -->
      </div>
    </div>
    <!--end: Row-->
  </div>
  <!--end: Container-->
</div>
<!-- end: Wrapper  -->
